    "grpc-tools": "^1.8.1",
    "grpc_tools_node_protoc_ts": "^2.5.10",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.3",
    "@prisma/client": "2.0.0-beta.4",
    "body-parser": "^1.19.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "google-protobuf": "^3.11.4",
    "grpc": "^1.24.2",
    "jsonwebtoken": "^8.5.1",
    "password-hash": "^1.2.2"
  }
